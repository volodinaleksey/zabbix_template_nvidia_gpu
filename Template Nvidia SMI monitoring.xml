<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-05-27T16:16:22Z</date>
    <groups>
        <group>
            <name>Templates/Hardware/GPU</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template Nvidia SMI monitoring</template>
            <name>Template Nvidia SMI monitoring</name>
            <groups>
                <group>
                    <name>Templates/Hardware/GPU</name>
                </group>
            </groups>
            <discovery_rules>
                <discovery_rule>
                    <name>NVIDIA GPUs discovery</name>
                    <key>nvidia.gpu.discovery</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#GPU.NAME} Clocks Graphics</name>
                            <type>DEPENDENT</type>
                            <key>clocks.graphics[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MHz</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Graphics\s+: (\w+) MHz
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Clocks Memory</name>
                            <type>DEPENDENT</type>
                            <key>clocks.memory[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MHz</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Memory\s+: (\w+) MHz
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Clocks SM</name>
                            <type>DEPENDENT</type>
                            <key>clocks.sm[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MHz</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>SM\s+: (\w+) MHz
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Clocks Video</name>
                            <type>DEPENDENT</type>
                            <key>clocks.video[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MHz</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Video\s+: (\w+) MHz
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} GPU Current Temp</name>
                            <type>DEPENDENT</type>
                            <key>gpu.current.temp[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!°C</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>GPU Current Temp\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} GPU Slowdown Temp</name>
                            <type>DEPENDENT</type>
                            <key>gpu.slowdown.temp[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!°C</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>GPU Slowdown Temp\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Memory free</name>
                            <type>DEPENDENT</type>
                            <key>memory.free[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MiB</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Free\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Memory total</name>
                            <type>DEPENDENT</type>
                            <key>memory.total[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MiB</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Total\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Memory used</name>
                            <type>DEPENDENT</type>
                            <key>memory.used[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!MiB</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Used\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>UUID:{#GPU.UUID} raw data</name>
                            <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            <history>0</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Power Default Limit</name>
                            <type>DEPENDENT</type>
                            <key>power.dpl[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!W</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Default Power Limit\s+: (\w+...)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Power Max Limit</name>
                            <type>DEPENDENT</type>
                            <key>power.mpl[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!W</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Max Power Limit\s+: (\w+...)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Power Draw</name>
                            <type>DEPENDENT</type>
                            <key>power.pd[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!W</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Power Draw\s+: (\w+...)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Fan Sped in %</name>
                            <type>DEPENDENT</type>
                            <key>temperature.gpu[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!%</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Fan Speed\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} GPU utilization in %</name>
                            <type>DEPENDENT</type>
                            <key>utilization.gpu[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!%</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Gpu\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#GPU.NAME} Memory utilization in %</name>
                            <type>DEPENDENT</type>
                            <key>utilization.memory[{#GPU.UUID}]</key>
                            <delay>0</delay>
                            <units>!%</units>
                            <preprocessing>
                                <step>
                                    <type>REGEX</type>
                                    <params>Memory\s+: (\w+)
\1</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>nvidia.gpu.getalldata[{#GPU.UUID}]</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <preprocessing>
                        <step>
                            <type>JAVASCRIPT</type>
                            <params>var lld = [];
var lines = value.split(/, |\r\n/);
var lines_num = lines.length;
for (i = 0; i &lt; lines_num -1; i+=2)
{
  var row = {};
  row[&quot;{#GPU.UUID}&quot;] = lines[i];
  row[&quot;{#GPU.NAME}&quot;] = lines[i+1];
  lld.push(row);
}
return JSON.stringify(lld); </params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
